.container[style="margin-top:1.5rem;"]
  .row
    .col.s12.m12.l12
      = form_for @post, :url => {:action => 'create'} do |f|
        - @post.errors.full_messages.each do |message|
          div
            = message
        = f.text_field :content
        = f.file_field :image
        input[type="submit" value="投稿"]
